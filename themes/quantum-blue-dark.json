        "editorError.foreground": "#FF5C7A",
        "editorWarning.foreground": "#FFC857",
        "editorInfo.foreground": "#38A1FF",
        "editorHint.foreground": "#32C0C8",
        "problemsErrorIcon.foreground": "#FF5C7A",
        "problemsWarningIcon.foreground": "#FFC857",
        "problemsInfoIcon.foreground": "#38A1FF"
    },
    "tokenColors": [
        {
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#7B8BA5",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "keyword",
                "keyword.control",
                "keyword.operator.new",
                "keyword.operator.expression",
                "keyword.operator.logical",
                "storage.type",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#38A1FF"
            }
        },
        {
            "scope": [
                "string",
                "string.quoted",
                "string.template"
            ],
            "settings": {
                "foreground": "#32C0C8"
            }
        },
        {
            "scope": [
                "constant.numeric",
                "constant.language.boolean",
                "constant.language.null",
                "constant.language.undefined"
            ],
            "settings": {
                "foreground": "#3FD6FF"
            }
        },
        {
            "scope": [
                "entity.name.function",
                "support.function",
                "meta.function-call"
            ],
            "settings": {
                "foreground": "#6EC8FF"
            }
        },
        {
            "scope": [
                "variable",
                "variable.parameter",
                "variable.other",
                "meta.definition.variable"
            ],
            "settings": {
                "foreground": "#D7E3FF"
            }
        },
        {
            "scope": [
                "entity.name.type",
                "entity.name.class",
                "support.type",
                "support.class",
                "entity.name.interface"
            ],
            "settings": {
                "foreground": "#8C9CFF"
            }
        },
        {
            "scope": [
                "constant.language",
                "variable.other.constant",
                "variable.other.enummember",
                "support.constant"
            ],
            "settings": {
                "foreground": "#35C9A8"
            }
        },
        {
            "scope": [
                "entity.name.tag",
                "meta.tag"
            ],
            "settings": {
                "foreground": "#38A1FF"
            }
        },
        {
            "scope": [
                "entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#6EC8FF"
            }
        },
        {
            "scope": [
                "punctuation",
                "meta.brace",
                "punctuation.definition"
            ],
            "settings": {
                "foreground": "#A0A8BF"
            }
        },
        {
            "scope": [
                "keyword.operator",
                "keyword.operator.arithmetic",
                "keyword.operator.assignment",
                "keyword.operator.comparison"
            ],
            "settings": {
                "foreground": "#3FD6FF"
            }
        },
        {
            "scope": [
                "support.type.property-name",
                "meta.object-literal.key"
            ],
            "settings": {
                "foreground": "#D7E3FF"
            }
        },
        {
            "scope": [
                "variable.language.this",
                "variable.language.super"
            ],
            "settings": {
                "foreground": "#8C9CFF",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "markup.heading"
            ],
            "settings": {
                "foreground": "#38A1FF",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "markup.bold"
            ],
            "settings": {
                "foreground": "#E2E6F0",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "markup.italic"
            ],
            "settings": {
                "foreground": "#E2E6F0",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "markup.inline.raw",
                "markup.fenced_code"
            ],
            "settings": {
                "foreground": "#32C0C8"
            }
        },
        {
            "scope": [
                "markup.list"
            ],
            "settings": {
                "foreground": "#6EC8FF"
            }
        },
        {
            "scope": [
                "markup.underline.link"
            ],
            "settings": {
                "foreground": "#38A1FF",
                "fontStyle": "underline"
            }
        },
        {
            "scope": [
                "invalid",
                "invalid.illegal"
            ],
            "settings": {
                "foreground": "#FF5C7A"
            }
        },
        {
            "scope": [
                "invalid.deprecated"
            ],
            "settings": {
                "foreground": "#FFC857"
            }
        }
    ]
}
